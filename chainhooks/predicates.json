{
  "chainhooks": [
    {
      "uuid": "passkey-wallet-deposit-events",
      "name": "Passkey Wallet Deposit Events",
      "version": 1,
      "networks": {
        "testnet": {
          "if_this": {
            "scope": "print_event",
            "contract_identifier": "SP1PQHQKV0RJXZFY1DGX8MNSNYVE3VGZJSRCBGD7R.passkey-wallet",
            "contains": "event: deposit"
          },
          "then_that": {
            "http_post": {
              "url": "${WEBHOOK_URL}/deposit",
              "authorization_header": "Bearer ${CHAINHOOK_API_KEY}"
            }
          },
          "start_block": 0
        }
      }
    },
    {
      "uuid": "passkey-wallet-withdraw-events",
